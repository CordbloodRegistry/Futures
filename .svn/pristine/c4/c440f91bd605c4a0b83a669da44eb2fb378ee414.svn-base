#import "cbrMapViewAnnotation.h"

@implementation cbrMapViewAnnotation

@synthesize title, coordinate;

- (id)initWithTitle:(NSString *)ttl andCoordinate:(CLLocationCoordinate2D)c2d {
    self = [super init];
    if (self != nil)
    {
        title = ttl;
        coordinate = c2d;
    }
	return self;
}

@end
